import java.util.Arrays;
/**
 * 这题不用分那么多种情况，因为只有两种结果，返回两种结果中大的即可
 * @author asus
 *
 */
public class t628 {
	class Solution {
	    public int maximumProduct(int[] nums) {
	        //sort first
	        Arrays.sort(nums);
	        int len = nums.length;
	        return Math.max(nums[len - 1] * nums[len - 2] * nums[len - 3] , nums[len - 1] * nums[0] * nums[1]);

	    }
	}
}
